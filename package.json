        "dotenv": "^16.4.5",
        "minimist": "^1.2.8",
        "node-localstorage": "^3.0.5",
        "node-os-utils": "^1.3.7",
        "secp256k1": "^5.0.0",
        "ts-node": "^10.9.2",
        "winston": "^3.13.0",
        "workerpool": "^9.1.1"
    },
    "devDependencies": {
        "@total-typescript/ts-reset": "^0.5.1",
        "@types/jest": "^29.5.12",
        "@types/minimist": "^1.2.5",
        "@types/node": "^20.12.7",
        "@types/node-localstorage": "^1.3.3",
        "@types/node-os-utils": "^1.3.4",
        "@types/secp256k1": "^4.0.6",
