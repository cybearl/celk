        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "jest": "^29.7.0",
        "jest-cli": "^29.7.0",
        "ts-jest": "^29.1.2",
        "tsc-alias": "^1.8.8",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.4.5"
