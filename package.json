{
    "name": "celk",
    "version": "2024.9.0-alpha",
    "description": "A brute-forcing tool for Bitcoin and Ethereum addresses entirely written in Typescript.",
    "repository": "https://github.com/cybearl/celk",
    "author": "yoratoni <yoratoni.dev@gmail.com>",
    "license": "MIT",
    "private": true,
    "type": "module",
    "scripts": {
        "migration:fresh": "cross-env NO_LIFECYCLE=true node ace migration:refresh --seed",
        "migration:run": "cross-env NO_LIFECYCLE=true node ace migration:run",
        "db:wipe": "cross-env NO_LIFECYCLE=true node ace db:wipe",
        "db:seed": "cross-env NO_LIFECYCLE=true node ace db:seed",
        "generate:key": "cross-env NO_LIFECYCLE=true node ace generate:key",
        "start": "node bin/server.js",
        "build": "node ace build",
        "dev": "node ace serve -w",
        "test": "node ace test",
        "test:bench": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./tests/bench.ts",
        "test:unit": "node ace test unit",
        "test:functional": "node ace test functional",
        "sandbox": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./tests/sandbox.ts",
        "lint": "eslint .",
        "format": "prettier --write .",
        "check-types": "tsc --noEmit"
    },
    "imports": {
        "#config/*": "./config/*.js",
        "#database/*": "./database/*.js",
        "#kernel/*": "./kernel/*.js",
        "#lib/*": "./lib/*.js",
        "#start/*": "./start/*.js",
        "#tests/*": "./tests/*.js",
        "#types/*": "./types/*.js",
        "#abilities/*": "./app/abilities/*.js",
        "#controllers/*": "./app/controllers/*.js",
        "#exceptions/*": "./app/exceptions/*.js",
        "#middleware/*": "./app/middleware/*.js",
        "#models/*": "./app/models/*.js",
        "#policies/*": "./app/policies/*.js",
        "#providers/*": "./app/providers/*.js",
        "#queues/*": "./app/queues/*.js",
        "#validators/*": "./app/validators/*.js",
        "#workers/*": "./app/workers/*.js"
    },
    "dependencies": {
        "@adonisjs/auth": "^9.2.3",
        "@adonisjs/core": "^6.12.1",
        "@adonisjs/cors": "^2.2.1",
        "@adonisjs/lucid": "^21.2.0",
        "@noble/hashes": "^1.4.0",
        "@vinejs/vine": "^2.1.0",
        "adonis-autoswagger": "^3.63.0",
        "bullmq": "^5.12.11",
        "dedent-js": "^1.0.1",
        "ethers": "^6.13.2",
        "luxon": "^3.5.0",
        "minimist": "^1.2.8",
        "pg": "^8.12.0",
        "reflect-metadata": "^0.2.2",
        "ts-node": "^10.9.2"
    },
    "devDependencies": {
        "@adonisjs/assembler": "^7.7.0",
        "@adonisjs/eslint-config": "^1.3.0",
        "@adonisjs/prettier-config": "^1.3.0",
        "@adonisjs/tsconfig": "^1.3.0",
        "@japa/api-client": "^2.0.3",
        "@japa/assert": "^3.0.0",
        "@japa/expect": "^3.0.2",
        "@japa/plugin-adonisjs": "^3.0.1",
        "@japa/runner": "^3.1.4",
        "@swc/core": "^1.7.21",
        "@types/luxon": "^3.4.2",
        "@types/minimist": "^1.2.5",
        "@types/node": "^20.14.12",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "hot-hook": "^0.2.6",
        "logform": "^2.6.1",
        "openapi-types": "^12.1.3",
        "pino-pretty": "^11.2.2",
        "prettier": "^3.3.3",
        "typescript": "^5.5.4",
        "winston": "^3.14.2"
    },
    "hotHook": {
        "boundaries": [
            "./app/controllers/**/*.ts",
            "./app/middleware/*.ts"
        ]
    },
    "eslintConfig": {
        "extends": "@adonisjs/eslint-config/app"
    }
}