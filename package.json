{
    "name": "celk",
    "version": "2024.12.2-alpha",
    "description": "A brute-forcing tool for Bitcoin and Ethereum addresses entirely written in Typescript.",
    "repository": "https://github.com/cybearl/celk",
    "author": "yoratoni <yoratoni.dev@gmail.com>",
    "license": "MIT",
    "private": true,
    "type": "module",
    "scripts": {
        "migration:fresh": "cross-env NO_LC=true node ace migration:refresh --seed",
        "migration:run": "cross-env NO_LC=true node ace migration:run",
        "db:wipe": "cross-env NO_LC=true node ace db:wipe",
        "db:seed": "cross-env NO_LC=true node ace db:seed",
        "generate:key": "cross-env NO_LC=true node ace generate:key",
        "dev": "node ace serve --watch",
        "start": "node bin/server.js",
        "build": "cross-env NO_LC=true node ace build",
        "benchmark": "tsx ./tests/benchmark.ts",
        "sandbox": "tsx ./tests/sandbox.ts",
        "test:unit": "node ace test unit",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "check-types": "tsc --noEmit"
    },
    "imports": {
        "#config/*": "./config/*.js",
        "#database/*": "./database/*.js",
        "#kernel/*": "./kernel/*.js",
        "#lib/*": "./lib/*.js",
        "#start/*": "./start/*.js",
        "#tests/*": "./tests/*.js",
        "#types/*": "./types/*.js",
        "#controllers/*": "./app/controllers/*.js",
        "#exceptions/*": "./app/exceptions/*.js",
        "#middleware/*": "./app/middleware/*.js",
        "#models/*": "./app/models/*.js",
        "#policies/*": "./app/policies/*.js",
        "#providers/*": "./app/providers/*.js",
        "#queues/*": "./app/queues/*.js",
        "#validators/*": "./app/validators/*.js",
        "#workers/*": "./app/workers/*.js"
    },
    "dependencies": {
        "@adonisjs/auth": "^9.3.0",
        "@adonisjs/bouncer": "^3.1.4",
        "@adonisjs/core": "^6.17.0",
        "@adonisjs/cors": "^2.2.1",
        "@adonisjs/lucid": "^21.5.1",
        "@cybearl/cypack": "^2024.11.6",
        "@noble/hashes": "^1.6.1",
        "@vinejs/vine": "^3.0.0",
        "bullmq": "^5.34.3",
        "dedent-js": "^1.0.1",
        "ethers": "^6.13.4",
        "ioredis": "^5.4.1",
        "luxon": "^3.5.0",
        "minimist": "^1.2.8",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.2.2",
        "secp256k1": "^5.0.1",
        "uuid": "^11.0.3"
    },
    "devDependencies": {
        "@adonisjs/assembler": "^7.8.2",
        "@adonisjs/eslint-config": "^1.3.0",
        "@adonisjs/prettier-config": "^1.4.0",
        "@adonisjs/tsconfig": "^1.4.0",
        "@japa/api-client": "^2.0.4",
        "@japa/assert": "^4.0.0",
        "@japa/expect": "^3.0.2",
        "@japa/plugin-adonisjs": "^3.0.1",
        "@japa/runner": "^3.1.4",
        "@swc/core": "^1.10.1",
        "@types/luxon": "^3.4.2",
        "@types/minimist": "^1.2.5",
        "@types/node": "^22.10.2",
        "@types/secp256k1": "^4.0.6",
        "@types/uuid": "^10.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.1",
        "hot-hook": "^0.4.0",
        "logform": "^2.7.0",
        "openapi-types": "^12.1.3",
        "pino-pretty": "^13.0.0",
        "prettier": "^3.4.2",
        "ts-node": "^10.9.2",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2",
        "winston": "^3.17.0"
    },
    "hotHook": {
        "boundaries": [
            "./app/controllers/**/*.ts",
            "./app/middleware/*.ts"
        ]
    },
    "eslintConfig": {
        "extends": "@adonisjs/eslint-config/app"
    }
}
